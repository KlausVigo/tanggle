isbn = {1076-836X (Electronic)$\backslash$r1063-5157 (Linking)},
issn = {1063-5157},
journal = {Systematic Biology},
keywords = {2-SAT,Algorithm,Antichain,Phylogenetic network,Phylogenetic tree,Reticulate evolution},
month = {sep},
number = {5},
pages = {768--777},
pmid = {26070685},
title = {{Which Phylogenetic Networks are Merely Trees with Additional Arcs?}},
url = {https://academic.oup.com/sysbio/article-lookup/doi/10.1093/sysbio/syv037},
volume = {64},
year = {2015}
}
