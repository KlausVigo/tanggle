pages = {e1002660},
pmid = {22536161},
title = {The Probability of a Gene Tree Topology within a Phylogenetic Network with Applications to Hybridization Detection},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3330115{\%}7B{\&}{\%}7Dtool=pmcentrez{\%}7B{\&}{\%}7Drendertype=abstract http://dx.plos.org/10.1371/journal.pgen.1002660 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3330115{\%}7B{\%}25{\%}7D7B{\%}7B{\&}{\%}7D{\%}7B{\%}25{\%}7D7D},
